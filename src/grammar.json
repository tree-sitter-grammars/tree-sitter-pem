{
  "name": "pem",
  "rules": {
    "pem": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "content"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          }
        ]
      }
    },
    "content": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "SYMBOL",
          "name": "data"
        },
        {
          "type": "SYMBOL",
          "name": "footer"
        }
      ]
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dashes"
        },
        {
          "type": "STRING",
          "value": "BEGIN"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "dashes"
        }
      ]
    },
    "footer": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dashes"
        },
        {
          "type": "STRING",
          "value": "END"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "dashes"
        }
      ]
    },
    "data": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z0-9+/]+={0,2}"
      }
    },
    "label": {
      "type": "PATTERN",
      "value": "[^-]+"
    },
    "dashes": {
      "type": "STRING",
      "value": "-----"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -2,
        "content": {
          "type": "PATTERN",
          "value": ".+"
        }
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
